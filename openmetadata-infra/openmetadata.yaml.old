database:
  driverClass: org.postgresql.Driver
  maxWaitForConnection: 1000
  validationQuery: "select 1"
  minSize: 1
  maxSize: 10
  checkConnectionHealthWhenIdle: false
  properties:
    autoCommit: false
    socketTimeout: 50000
    rewriteBatchedStatements: true
  url: jdbc:postgresql://om_postgres:5432/openmetadata?sslmode=disable
  user: openmetadata_user
  password: openmetadata_password
  logSql: false
  connectionTimeout: 50000

elasticsearch:
  host: opensearch
  port: 9200
  scheme: http
  username: opensearch
  password: opensearch
  indexType: elasticSearch
  searchIndexMappingLanguage: EN
  connectionTimeout: 50000
  socketTimeout: 50000

authenticationConfiguration:
  provider: basic
  publicKeyUrls:
    - http://localhost:8585/api/v1/auth/token/publickey
