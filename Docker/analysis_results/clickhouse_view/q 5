docker exec clickhouse clickhouse-client --query "
>> WITH yearly_data AS (
>>     SELECT
>>         toYear(toDate(date_added)) as year,
>>         gics_sector as sector,
>>         count(DISTINCT symbol) as company_count
>>     FROM sp600_stocks.sp500_components
>>     WHERE toYear(toDate(date_added)) BETWEEN 2000 AND 2025
>>     AND gics_sector != ''
>>     GROUP BY year, sector
>> ),
>> yearly_totals AS (
>>     SELECT
>>         year,
>>         sum(company_count) as total_companies
>>     FROM yearly_data
>>     GROUP BY year
>> )
>> SELECT
>>     y.year as Year,
>>     y.sector as Sector,
>>     y.company_count,
>>     ROUND(y.company_count * 100.0 / t.total_companies, 2) as percentage
>> FROM yearly_data y
>> JOIN yearly_totals t ON y.year = t.year
>> WHERE y.sector IN ('Information Technology', 'Health Care', 'Financials', 'Industrials', 'Energy')
>> ORDER BY y.year, y.company_count DESC
>> FORMAT Pretty"
┏━━━━━━┳━━━━━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━┓
┃ Year ┃ Sector                 ┃ company_count ┃ percentage ┃
┡━━━━━━╇━━━━━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━╇━━━━━━━━━━━━┩
