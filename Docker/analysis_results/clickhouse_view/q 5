docker exec clickhouse clickhouse-client --query "
>> WITH sector_2000 AS (
>>     SELECT
>>         gics_sector as sector,
>>         count(DISTINCT symbol) as company_count_2000
>>     FROM sp600_stocks.sp500_components
>>     WHERE toYear(toDate(date_added)) = 2000
>>     AND gics_sector != ''
>>     GROUP BY gics_sector
>> ),
>> sector_2025 AS (
>>     SELECT
>>         gics_sector as sector,
>>         count(DISTINCT symbol) as company_count_2025
>>     FROM sp600_stocks.sp500_components
>>     WHERE toYear(toDate(date_added)) = 2025
>>     AND gics_sector != ''
>>     GROUP BY gics_sector
>> ),
>> total_2000 AS (SELECT sum(company_count_2000) as total FROM sector_2000),
>> total_2025 AS (SELECT sum(company_count_2025) as total FROM sector_2025)
>> SELECT
>>     COALESCE(s2000.sector, s2025.sector) as Sector,
>>     s2000.company_count_2000 as Count_2000,
>>     ROUND(s2000.company_count_2000 * 100.0 / t2000.total, 2) as Percent_2000,
>>     s2025.company_count_2025 as Count_2025,
>>     ROUND(s2025.company_count_2025 * 100.0 / t2025.total, 2) as Percent_2025,
>>     COALESCE(s2025.company_count_2025, 0) - COALESCE(s2000.company_count_2000, 0) as Change_Count,
>>     ROUND(
>>         (COALESCE(s2025.company_count_2025, 0) - COALESCE(s2000.company_count_2000, 0)) * 100.0 /
>>         NULLIF(COALESCE(s2000.company_count_2000, 1), 0), 2
>>     ) as Percent_Change
>> FROM sector_2000 s2000
>> FULL OUTER JOIN sector_2025 s2025 ON s2000.sector = s2025.sector
>> CROSS JOIN total_2000 t2000
>> CROSS JOIN total_2025 t2025
>> ORDER BY COALESCE(s2025.company_count_2025, 0) DESC, COALESCE(s2000.company_count_2000, 0) DESC
>> FORMAT Pretty"
┏━━━━━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━┳━━━━━━━━━━━━━━┳━━━━━━━━━━━━┳━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━━┓
┃ Sector                 ┃ Count_2000 ┃ Percent_2000 ┃ Count_2025 ┃ Percent_2025 ┃ Change_Count ┃ Percent_Change ┃
┡━━━━━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━╇━━━━━━━━━━━━━━╇━━━━━━━━━━━━╇━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━━┩
