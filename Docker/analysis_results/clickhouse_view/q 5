docker exec clickhouse clickhouse-client --query "
>> WITH decade_data AS (
>>     SELECT
>>         CASE
>>             WHEN year_added BETWEEN 2000 AND 2004 THEN '2000-2004'
>>             WHEN year_added BETWEEN 2005 AND 2009 THEN '2005-2009'
>>             WHEN year_added BETWEEN 2010 AND 2014 THEN '2010-2014'
>>             WHEN year_added BETWEEN 2015 AND 2019 THEN '2015-2019'
>>             WHEN year_added BETWEEN 2020 AND 2025 THEN '2020-2025'
>>         END as period,
>>         gics_sector as sector,
>>         count(DISTINCT symbol) as company_count
>>     FROM (
>>         SELECT
>>             symbol,
>>             gics_sector,
>>             toYear(toDate(date_added)) as year_added
>>         FROM sp600_stocks.sp500_components
>>         WHERE toYear(toDate(date_added)) BETWEEN 2000 AND 2025
>>         AND gics_sector != ''
>>     )
>>     GROUP BY period, sector
>> ),
>> period_totals AS (
>>     SELECT
>>         period,
>>         sum(company_count) as total_companies
>>     FROM decade_data
>>     GROUP BY period
>> )
>> SELECT
>>     d.period as Period,
>>     d.sector as Sector,
>>     d.company_count,
>>     ROUND(d.company_count * 100.0 / p.total_companies, 2) as percentage
>> FROM decade_data d
>> JOIN period_totals p ON d.period = p.period
>> ORDER BY
>>     CASE d.period
>>         WHEN '2000-2004' THEN 1
>>         WHEN '2005-2009' THEN 2
>>         WHEN '2010-2014' THEN 3
>>         WHEN '2015-2019' THEN 4
>>         WHEN '2020-2025' THEN 5
>>     END,
>>     d.company_count DESC
>> FORMAT Pretty"
┏━━━━━━━━━━━┳━━━━━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━┳━━━━━━━━━━━━┓
┃ Period    ┃ Sector                 ┃ company_count ┃ percentage ┃
┡━━━━━━━━━━━╇━━━━━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━╇━━━━━━━━━━━━┩
