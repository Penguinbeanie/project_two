docker exec clickhouse clickhouse-client --query "
>> WITH sector_yearly AS (
>>     SELECT
>>         toYear(toDate(date_added)) as year,
>>         gics_sector as sector,
>>         count(DISTINCT symbol) as company_count
>>     FROM sp600_stocks.sp500_components
>>     WHERE toYear(toDate(date_added)) BETWEEN 2000 AND 2025
>>     AND gics_sector != ''
>>     GROUP BY year, sector
>> ),
>> sector_growth AS (
>>     SELECT
>>         sector,
>>         SUM(CASE WHEN year BETWEEN 2000 AND 2004 THEN company_count ELSE 0 END) as count_2000_2004,
>>         SUM(CASE WHEN year BETWEEN 2021 AND 2025 THEN company_count ELSE 0 END) as count_2021_2025,
>>         COUNT(DISTINCT year) as years_present
>>     FROM sector_yearly
>>     GROUP BY sector
>>     HAVING years_present >= 3  -- Sectors present in at least 3 different years
>> )
>> SELECT
>>     sector as Sector,
>>     count_2000_2004 as Count_Early_2000s,
>>     count_2021_2025 as Count_Recent_2020s,
>>     count_2021_2025 - count_2000_2004 as Net_Change,
>>     ROUND(
>>         (count_2021_2025 - count_2000_2004) * 100.0 /
>>         NULLIF(count_2000_2004, 0), 2
>>     ) as Percent_Change,
>>     CASE
>>         WHEN count_2021_2025 > count_2000_2004 THEN 'Growing'
>>         WHEN count_2021_2025 < count_2000_2004 THEN 'Declining'
>>         ELSE 'Stable'
>>     END as Trend
>> FROM sector_growth
>> ORDER BY Net_Change DESC
>> FORMAT Pretty"
┏━━━━━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━━━━━━━━━┳━━━━━━━━━━━━┳━━━━━━━━━━━━━━━━┳━━━━━━━━━━━┓
┃ Sector                 ┃ Count_Early_2000s ┃ Count_Recent_2020s ┃ Net_Change ┃ Percent_Change ┃ Trend     ┃
┡━━━━━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━━━━━━━━━╇━━━━━━━━━━━━╇━━━━━━━━━━━━━━━━╇━━━━━━━━━━━┩
